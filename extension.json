{
    "name": "ASBSearch",
    "version": "0.1.0",
    "author": [
        "Shocks"
    ],
    "url": "none",
    "descriptionmsg": "Database search tool for ASB related SQL data.",
    "license-name": "MIT",
    "type": "other",
    "AutoloadClasses": {
        "SpecialASBSearch": "includes/Special.php",
        "ExclusionsHelper": "includes/ExclusionsHelper.php",
        "ParserHelper": "includes/ParserHelper.php"
	},
    "SpecialPages": {
        "ASBSearch": "SpecialASBSearch"
    },
	"ExtensionMessagesFiles": {
		"ASBSearchAlias": "i18n/ASBSearch.i18n.alias.php"
	},
	"ResourceModules": {
		"inputHandler" : {
			"localBasePath": "includes/",
			"remoteExtPath": "ASBSearch/includes/",
			"packageFiles": [
				"inputHandler.js"
			],
			"styles": [
				"special.css"
			]
		}
	},
    "Hooks": {
        "BeforePageDisplay": "SpecialASBSearch::onBeforePageDisplay"
    },
    "DatabaseVirtualDomains": [
        "testASB"
    ],
    "manifest_version": 2,
    "requires": {
        "MediaWiki": ">= 1.39.0",
        "platform": {
            "php": ">= 5.6"
        }

    }
}